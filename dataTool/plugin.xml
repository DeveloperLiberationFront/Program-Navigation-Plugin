<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="dataTool.actionSet1"
            label="dataTool.actionSet1"
            visible="true">
         <menu
               id="dataTool.menu2"
               label="TempMenu">
            <groupMarker name="TempGroup"/>
         </menu>
         <action
               class="dataTool.EnableNavigationAction"
               icon="icons/data.gif"
               id="dataTool.action1"
               label="Find EOS"
               style="toggle"
               toolbarPath="TempGroup"
               tooltip="Highlights flow of data within your code"/>
      </actionSet>
      <command id="org.eclipse.example.command.toggle"
         name="Toggle Me">
	  <state class="org.eclipse.jface.commands.ToggleState"
	         id="org.eclipse.example.command.toggleState:true" />
	</command>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
   </extension>
</plugin>
